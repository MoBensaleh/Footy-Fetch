{
    "version": 2,
    "builds": [
      {
        "src": "./server/server.ts",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/build", 
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server/server.ts"
      }
    ]
  }
  